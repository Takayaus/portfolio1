<div class="container">
    <div class="gym-title">
        <h3><%= @gym.name %></h3>
        <p><%= @gym.title %></p>
        <p><%= @gym.number %></p>
    </div>
    <%= render 'gyms/gyms-nav' %>
    <div class="gym-content">
        <%= @gym.content %>
    </div>
    <% @gym.item_images.each do |item_image| %>
        <%= image_tag item_image.image.url %>
    <% end %>
</div>
